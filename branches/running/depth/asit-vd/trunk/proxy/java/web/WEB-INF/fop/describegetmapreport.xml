	<ProcessDescription processVersion="1" storeSupported="false" statusSupported="false">
		<ows:Identifier>getMapReport</ows:Identifier>
		<ows:Title>Get a map report</ows:Title>
		<ows:Abstract>Get a report which includes a map image and transforms a WFS query response into a list format.</ows:Abstract>
		<ows:Metadata xlink:title="WFS" />
		<ows:Metadata xlink:title="Report" />
		<DataInputs>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>userName</ows:Identifier>
				<ows:Title>Easysdi provider's username</ows:Title>
				<ows:Abstract>Easysdi provider's username</ows:Abstract>
				<LiteralData>
			       <ows:DataType ows:reference="urn:ogc:def:dataType:OGC:1.0:String">String</ows:DataType>
			    </LiteralData>
			</Input>			
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>filters</ows:Identifier>
				<ows:Title>Filters</ows:Title>
				<ows:Abstract>Text description of filters applied for report query.</ows:Abstract>
				<LiteralData>
			       <ows:DataType ows:reference="urn:ogc:def:dataType:OGC:1.0:String">String</ows:DataType>
			    </LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>type</ows:Identifier>
				<ows:Title>Type of report</ows:Title>
				<ows:Abstract>Type of report, e.g. simple species or advanced habitats</ows:Abstract>
				<LiteralData>
			       <ows:DataType ows:reference="urn:ogc:def:dataType:OGC:1.0:String">String</ows:DataType>
			    </LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>spatial</ows:Identifier>
				<ows:Title>Spatial report</ows:Title>
				<ows:Abstract>Set to 1 if report has spatial filter, else 0</ows:Abstract>
				<LiteralData>
			       <ows:DataType ows:reference="urn:ogc:def:dataType:OGC:1.0:boolean">Boolean</ows:DataType>
			    </LiteralData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>wfsRequest</ows:Identifier>
				<ows:Title>WFS post request</ows:Title>
				<ows:Abstract>Post request as it would be forwarded direct to the EasySDI WFS proxy server.</ows:Abstract>
				<ComplexData>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/wfs/1.1.0/wfs.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/wfs/1.1.0/wfs.xsd</Schema>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
			<Input minOccurs="1" maxOccurs="1">
				<ows:Identifier>mapContext</ows:Identifier>
				<ows:Title>WMC map context</ows:Title>
				<ows:Abstract>WMC description of map to draw.</ows:Abstract>
				<ComplexData>
					<Default>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/context/1.0.0/context.xsd</Schema>
						</Format>
					</Default>
					<Supported>
						<Format>
							<MimeType>text/xml</MimeType>
							<Encoding>UTF-8</Encoding>
							<Schema>http://schemas.opengis.net/context/1.0.0/context.xsd</Schema>
						</Format>
					</Supported>
				</ComplexData>
			</Input>
		</DataInputs>
		<ProcessOutputs>
			<Output>
				<ows:Identifier>download</ows:Identifier>
				<ows:Title>Report download</ows:Title>
				<ows:Abstract>Downloadable report information in chosen format.</ows:Abstract>
				<ComplexOutput defaultFormat="text/csv" defaultEncoding="UTF-8" defaultSchema="http://foo.bar/gml/3.1.0/polygon.xsd">
					<SupportedComplexData>
						<MimeType>application/pdf</MimeType>
						<Encoding>UTF-8</Encoding>
					</SupportedComplexData>
				</ComplexOutput>
			</Output>
		</ProcessOutputs>
	</ProcessDescription>
