<?xml version="1.0" encoding="US-ASCII"?>
<xs:schema  xmlns:my2="http://www.mygeomelements.com" xmlns:my1="http://www.shapes.com"
   xmlns:xs="http://www.w3.org/2001/XMLSchema"
   targetNamespace="http://www.shapes.com"
   elementFormDefault="qualified">
   <xs:import namespace="http://www.mygeomelements.com" schemaLocation="./schema2.xsd"/>
   <xs:complexType name="rectanglesCollection" mixed="true">
      <xs:sequence>
         <xs:element name="rectangle" type="my1:recEntry" minOccurs="1" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="recEntry" mixed="true">
      <xs:sequence>
         <xs:element name="origin" type="my2:point" minOccurs="1" maxOccurs="1"/>
         <xs:element name="width" type="xs:positiveInteger" minOccurs="1" maxOccurs="1"/>
         <xs:element name="height" type="xs:positiveInteger" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute name="id" type="xs:positiveInteger"/>
   </xs:complexType>
   
   <xs:simpleType name="myInteger">      
      <xs:list itemType="xs:string"/>
   </xs:simpleType>

   <xs:complexType name="recEntry2">
      <xs:complexContent>
         <xs:extension base="my1:recEntry"/>
      </xs:complexContent>
   </xs:complexType>
<xs:simpleType name="myInteger2">
   <xs:restriction base="xs:integer"/>
</xs:simpleType>

   <xs:complexType name="myComplexInteger">
      <xs:simpleContent>
      <xs:extension base="xs:integer"/>
      </xs:simpleContent>
   </xs:complexType>
   
   <xs:simpleType name="integerUnion">
      <xs:union memberTypes="xs:integer my1:myInteger my1:myInteger2"/>
   </xs:simpleType>
</xs:schema>