<?xml version="1.0" encoding="UTF-8"?>
<ProcessDefinition configVersion="0.5.0" processVersion="1.0.0" storeSupported="true" statusSupported="true"
  xmlns="http://www.deegree.org/services/wps" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.deegree.org/services/wps http://schemas.deegree.org/wps/0.5.0/process_definition.xsd">
  <Identifier>TransformDataset</Identifier>
  <JavaClass>org.easysdi.publish.gui.wps.provider.TransformDatasetProcesslet</JavaClass>
  <Title>Process for uploading and transforming a dataset (FeatureSource) to the diffuser database.</Title>
  <Abstract>The purpose of this process is upload and transform a dataset from various format to an OWS diffuser database, so it can be then used for WMS/WFS publication.</Abstract>
  <InputParameters>
    <LiteralInput>
      <Identifier>DiffusionServerName</Identifier>
      <Title>Name of the target diffusion server</Title>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#string">string</DataType>
      <!--  <DefaultUOM>unity</DefaultUOM> -->
    </LiteralInput>
    <LiteralInput>
      <Identifier>FeatureSourceId</Identifier>
      <Title>Feature Source Id (only for updates; for new set to none)</Title>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#string">string</DataType>
      <!--  <DefaultUOM>unity</DefaultUOM> -->
    </LiteralInput>
    <LiteralInput minOccurs="0" maxOccurs="20">
      <Identifier>URLFile</Identifier>
      <Title>The URL of the file to transform</Title>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#string">string</DataType>
      <!--  <DefaultUOM>unity</DefaultUOM> -->
    </LiteralInput>
    <ComplexInput minOccurs="0" maxOccurs="1">
      <Identifier>ZipInput</Identifier>
      <Title>The file to transform compressed in a Zip file</Title>
      <DefaultFormat encoding="base64" mimeType="application/zip" />
    </ComplexInput>
    <LiteralInput>
      <Identifier>ScriptName</Identifier>
      <Title>Name of the script to execute</Title>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#string">string</DataType>
      <!--  <DefaultUOM>unity</DefaultUOM> -->
    </LiteralInput>
    <LiteralInput>
      <Identifier>SourceDataType</Identifier>
      <Title>Type of the data source (e.g SHAPE)</Title>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#string">string</DataType>
      <!--  <DefaultUOM>unity</DefaultUOM> -->
    </LiteralInput>
    <LiteralInput>
      <Identifier>CoordEpsgCode</Identifier>
      <Title>The EPSG code of the projection</Title>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#string">string</DataType>
      <!--  <DefaultUOM>unity</DefaultUOM> -->
    </LiteralInput>
    <LiteralInput>
      <Identifier>Dataset</Identifier>
      <Title>The dataset name within the file to transform</Title>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#string">string</DataType>
      <!--  <DefaultUOM>unity</DefaultUOM> -->
    </LiteralInput>
  </InputParameters>
  <OutputParameters>
    <LiteralOutput>
      <Identifier>FeatureSourceGuid</Identifier>
      <Title>The universal identifier of the created dataset</Title>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#string">string</DataType>
      <!--  <DefaultUOM>unity</DefaultUOM> -->
    </LiteralOutput>
  </OutputParameters>
</ProcessDefinition>
