<CONFIG>
        <!--  MYSQL -->
		<DB_MANUFACTURER>mysql</DB_MANUFACTURER>
		<DB_NAME>publish_server</DB_NAME>
		<!-- The HSQL driver used to connect to the DB -->
		<CONNECTION_DRIVER>com.mysql.jdbc.Driver</CONNECTION_DRIVER>
		<!-- The jdbc URL to the HSQLDB server -->
		<CONNECTION_URL>jdbc:mysql://localhost</CONNECTION_URL>
		<!-- The jdbc Username for the HSQLDB server -->
		<CONNECTION_USER>root</CONNECTION_USER>
		<!-- The jdbc password for the HSQLDB server -->
		<CONNECTION_PASSWORD></CONNECTION_PASSWORD>
		
		<!-- The default Publish configuration values at least needed
		for the application to run. Then we can edit the configuration
		at runtime.  -->
		<!-- The diffuser enums -->
		<diffuser_type>mapserver,geoserver</diffuser_type>
		<!-- The geodatabase enums -->
		<geodatabase_type>postgis</geodatabase_type>
		<!-- The transformator_type tool -->
		<transformator_type>fme,sdi,geokeetle</transformator_type>
		<!-- The transformator -->
		<transformator>MySDI,sdi</transformator>
		<!-- The default diffuser geodatabase -->
		<!-- dbname, url, username, password, schema, geodatabase_type -->
		<geodatabase>postgislocalhost,jdbc:postgresql://localhost:5432/postgis,postgres,mypassword,public,postgis</geodatabase>
		<!--  the default diffuser -->
		<!-- name, url, username, password, diffuser_type -->
		<diffuser>postgislocalhost,diffuserlocalhost,http://localhost:8080/geoserver,admin,geoserver,geoserver</diffuser>
		<!-- The layerstatus enum -->
		<layerstatus>published,unpublished,undefined</layerstatus>
		<!-- the main ogr script -->
		<!-- transformer, name, location, command, arguments, conditions, requiredFiles -->
		<script>MySDI,OGR, , , , , </script>
		<!-- List All File Types -->
		<!-- Handled File Types -->
		<SupportedTransformedFileTypes>MockType,OGR</SupportedTransformedFileTypes>
		<!-- Associate File Types With A Concrete Transformer-->
		<!-- Handled File Name -->
		<!-- Concrete Transformer -->
		<MockType>eu.bauel.publish.transformation.plugin.MockTransformer</MockType>
		<!-- Handled File Name -->
		<!-- Concrete Transformer -->
		<OGR>eu.bauel.publish.transformation.plugin.OGRTransformer</OGR>
