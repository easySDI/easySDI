<?xml version="1.0" encoding="US-ASCII"?>
<xs:schema xmlns:my2="http://www.mygeomelements.com"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    targetNamespace="http://www.mygeomelements.com"
    elementFormDefault="qualified">
    <xs:complexType name="point" mixed="true">
        <xs:sequence>
            <xs:element name="xcoord" type="xs:integer" minOccurs="1" maxOccurs="1"/>
            <xs:element name="ycoord" type="xs:integer" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>