	<config id="geonetworkcsw">
		<authorization>
			<policy-file>C:\uri_to_policy\NewPolicy.xml</policy-file>
		</authorization>
		<servlet-class>org.easysdi.proxy.csw.CSWProxyServlet</servlet-class>
		<log-config>
			<file-structure>
				<path>C:\uri_to_log\PROXY_log</path>
				<suffix>log_csw</suffix>
				<prefix>geonetwork</prefix>
				<extension>log</extension>
				<period>daily</period>
			</file-structure>
			<date-format>dd/MM/yyyy HH:mm:ss</date-format>
		</log-config>
		<host-translator></host-translator>
		<remote-server-list>
			<remote-server>
				<url>http://localhost:8080/geonetwork/srv/en/csw</url>
				<user>service_user</user>
				<password>user_pass</password>
				<max-records>50</max-records>
				<login-service>http://localhost:8080/geonetwork/srv/en/xml.user.login?username=admin&amp;password=admin</login-service>
				<transaction>
				<type>geonetwork</type>
				<search-service-url>http://localhost:8080/geonetwork/srv/en/xml.search</search-service-url>
				<delete-service-url>http://localhost:8080/geonetwork/srv/en/metadata.delete</delete-service-url>
				<insert-service-url>http://localhost:8080/geonetwork/srv/en/mef.import</insert-service-url>
				<update-service-url>http://localhost:8080/geonetwork/srv/en/metadata.update</update-service-url>
				</transaction>
			</remote-server>
		</remote-server-list>
	</config>
</configurations>
