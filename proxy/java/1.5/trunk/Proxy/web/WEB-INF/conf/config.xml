<?xml version="1.0" encoding="UTF-8"?>
<configurations>
	<config id="geoserverwms">
		<authorization>
			<policy-file>C:\uri_to_policy\NewPolicy.xml</policy-file>
		</authorization>
		<servlet-class>org.easysdi.proxy.wms.WMSProxyServlet</servlet-class>
		<log-config>
			<file-structure>
				<path>C:\uri_to_log\PROXY_log</path>
				<suffix>wms</suffix>
				<prefix>_easi</prefix>
				<extension>log</extension>
				<period>daily</period>
			</file-structure>
			<date-format>dd/MM/yyyy HH:mm:ss:SSS</date-format>
		</log-config>
		<host-translator />
		<remote-server-list>
			<remote-server>
				<user>service_user</user>
				<url>http://localhost:8080/geoserver/wms</url>
				<prefix>tiger</prefix>
				<password>user_pass</password>
			</remote-server>
		</remote-server-list>
	</config>
