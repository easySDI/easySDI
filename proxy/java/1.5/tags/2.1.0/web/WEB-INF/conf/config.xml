<?xml version="1.0" encoding="UTF-8"?>
<configurations>
	<config id="metacarta_open">
		<authorization>
			<policy-file>/var/lib/tomcat5/webapps/proxy/WEB-INF/conf/NewPolicy.xml
			</policy-file>
		</authorization>
		<servlet-class>org.easysdi.proxy.wms.WMSProxyServlet</servlet-class>
		<log-config>
			<file-structure>
				<path>/var/lib/tomcat5/webapps/proxy/WEB-INF/logs/</path>
				<suffix>wms</suffix>
				<prefix>metacarta</prefix>
				<extension>log</extension>
				<period>daily</period>
			</file-structure>
			<date-format>dd/MM/yyyy HH:mm:ss</date-format>
		</log-config>
		<host-translator />
		<remote-server-list>
			<remote-server>
				<user />
				<url>http://labs.metacarta.com/wms/vmap0</url>
				<password />
			</remote-server>
		</remote-server-list>
	</config>
