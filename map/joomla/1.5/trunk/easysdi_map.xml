<?xml version="1.0" ?>
<install type="component" version="1.5">
	<name>EasySDI_Map</name>
	<version>2.0.0</version>
	<releaseDate>2011-03-30</releaseDate>
    <releaseType>Major update</releaseType>
	<creationDate>Mars 2011</creationDate>
	<author>Depth.ch</author>
	<authorEmail>easysdi@depth.ch</authorEmail>
	<authorUrl>www.depth.ch</authorUrl>
	<authorUrl>http://www.easysdi.org</authorUrl>
	<copyright></copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<description>Web Mapping Module for EasySDI</description>
	<installfile>install.easysdi_map.php</installfile>
	<uninstallfile>uninstall.easysdi_map.php</uninstallfile>
	<administration>
		<files folder="administrator">
			<filename>admin.easysdi_map.php</filename>
			<folder>core</folder>
			<folder>tables</folder>
		</files>
		<languages folder="administrator/language">
			<language tag="fr-FR">fr-FR.com_easysdi_map.ini</language>
			<language tag="en-GB">en-GB.com_easysdi_map.ini</language>
		</languages>
	</administration>
	<files folder="components">
		<filename>easysdi_map.php</filename>
		<folder>assets</folder>
		<folder>classes</folder>
		<folder>controllers</folder>
		<folder>elements</folder>
		<folder>externals</folder>
		<folder>models</folder>
		<folder>php</folder>
		<folder>resource</folder>
		<folder>views</folder>
	</files>
	<languages folder="components/language">
		<language tag="fr-FR">fr-FR.com_easysdi_map.ini</language>
		<language tag="en-GB">en-GB.com_easysdi_map.ini</language>
	</languages>
</install>